<!DOCTYPE html>
<html>
<head>
  <% include ../partials/zach_header.ejs %>
</head>

<body id="background1">

  <% include ../partials/zach_nav.ejs %>
	<div class="forms">
		<form method="post" action="/edit" onkeypress="return event.keyCode != 13;">
			<div class="form-group">
				<label for="name">Name (First and Last): </label><br/>
				<input id="name" name="name" class="form-control" type="text" placeholder="<%= result[0]%>"></input>
			</div>
			<div class="form-group">
				<label for="address">Street Address: </label><br/>
				<input id="address" name="address" class="form-control" type="text" placeholder="<%= result[1]%>"></input>
			</div>
			<div class="form-group">
				<label for="city">City: </label><br/>
				<input id="city" name="city" class="form-control" type="text" placeholder="<%= result[2]%>"></input>
			</div>
			<div class="form-group">
				<label for="state">State: </label><br/>
				<input id="state" name="state" class="form-control" type="text" placeholder="<%= result[3]%>"></input>
			</div>
			<div class="form-group">
				<label for="zip">Zip: </label><br/>
				<input id="zip" name="zip" class="form-control" type="text" placeholder="<%= result[4]%>"></input>
			</div>
			<div class="form-group">
				<label for="email">Email: </label><br/>
				<input id="email" class="form-control" name="email" type="email" placeholder="<%= result[5]%>"></input>
			</div>
			<div class="form-group">
				<label for="phone">Phone: </label><br/>
				<input id="phone" name="phone" class="form-control" type="phone" placeholder="<%= result[6]%>"></input>
			</div>
			<div class="form-group">
				<label for="username">Username: </label><br/>
				<input id="username" class="form-control" name="username" type="text" placeholder="<%= result[7]%>"></input>
			</div>
			<div class="form-group">
				<label for="password">Password: </label><br/>
				<input id="password" class="form-control" name="password" type="password"></input><br/>
			</div>
			<div class="form-group">
				<input type="submit" class="btn btn-default" value="Update Account" onclick="return createAndEditValidate()" class="btn btn-default"></input><br/>
			</div>
		</form>
	</div>
	<div class="gap">
	</div>
	<% include ../partials/zach_footer.ejs %>
</body>
</html>