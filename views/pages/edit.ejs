<!DOCTYPE html>
<html>
<head>
  <% include ../partials/zach_header.ejs %>
</head>

<body id="background1">

  <% include ../partials/zach_nav.ejs %>
	<div class="forms">
		<form method="post" action="/edit" onkeypress="return event.keyCode != 13;">
			<input id="name" name="name" type="text" placeholder="<%= result[0]%>">Name</input><br/>
			<input id="address" name="address" type="text" placeholder="<%= result[1]%>">Address</input><br/>
			<input id="city" name="city" type="text" placeholder="<%= result[2]%>">City</input><br/>
			<input id="state" name="state" type="text" placeholder="<%= result[3]%>">State</input><br/>
			<input id="zip" name="zip" type="text" placeholder="<%= result[4]%>">Zip</input><br/>
			<input id="email" name="email" type="email" placeholder="<%= result[5]%>">Email</input><br/>
			<input id="phone" name="phone" type="phone" placeholder="<%= result[6]%>">Phone</input><br/>
			<input id="username" name="username" type="text" placeholder="<%= result[7]%>">Username</input><br/>
			<input id="password" name="password" type="password">Password</input><br/>
			<input type="submit" value="Create Account"></input><br/>
		</form>
	</div>
</body>
</html>